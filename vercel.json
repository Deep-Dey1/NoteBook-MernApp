{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "devCommand": "cd frontend && npm run dev",
  "installCommand": "cd backend && npm install",
  "framework": null,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/src/server.js"
    }
  ],
  "functions": {
    "backend/src/server.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
